{
  "expo": {
    "name": "In_Response",
    "slug": "In_Response",
    "version": "1.8",
    "orientation": "portrait",
    "icon": "./assets/appIcons/mtg_counter_icon_1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appIcons/Color_Wheel.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcons/adaptive_mtg_counter_icon_1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beedazzle.In_Response"
    },
    "web": {
      "favicon": "./assets/appIcons/adaptive_mtg_counter_icon_1024.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf756dd9-c496-4b10-9018-4229073b742a"
      }
    }
  }
}
