{
  "expo": {
    "name": "In_Response",
    "slug": "In_Response",
    "version": "4.1.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/appIcons/mtg_counter_icon_1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appIcons/splash/Color_Wheel.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcons/adaptive_mtg_counter_icon_1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.beedazzle.mtg_counter",
      "versionCode": 26
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/appIcons/splash/Color_Wheel.png",
          "dark": {
            "image": "./assets/appIcons/splash/Color_Wheel.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 400
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf756dd9-c496-4b10-9018-4229073b742a"
      }
    }
  }
}
